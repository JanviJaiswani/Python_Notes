x= "abc"
y="23"

#bool Function
print(bool(x))      #True
print(bool(y))      #True
print(bool("abc"))  #True      
print(bool(5))      #True
print(bool(False)) #false
print(bool(None))  #false
print(bool(0))     #false
print(bool(""))    #false
print(bool(()))    #false
print(bool([]))    #false
print(bool({}))    #false